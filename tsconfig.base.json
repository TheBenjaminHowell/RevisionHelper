    "compilerOptions": {
        "isolatedModules": true,
        "verbatimModuleSyntax": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "erasableSyntaxOnly": true,
        "noFallthroughCasesInSwitch": true,
        "noUncheckedSideEffectImports": true
